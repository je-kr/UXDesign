<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="50dp">

    <TextView
        android:id="@+id/textViewHeureAgendaPatient"
        android:layout_width="125dp"
        android:layout_height="28dp"
        android:gravity="center"
        android:text="HH:MM"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.171"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></TextView>

    <TextView
        android:id="@+id/textViewAdresseMedecinAgendaPatient"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adresse TEST Medecin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.935"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.967" />

    <TextView
        android:id="@+id/textViewNomMedecinAgendaPatient"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dr TEST"
        app:layout_constraintBottom_toTopOf="@+id/textViewAdresseMedecinAgendaPatient"
        app:layout_constraintEnd_toEndOf="@+id/textViewAdresseMedecinAgendaPatient"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textViewAdresseMedecinAgendaPatient"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.909" />

</androidx.constraintlayout.widget.ConstraintLayout>